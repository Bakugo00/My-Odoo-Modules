<?xml version="1.0"?>
<odoo>
    <data noupdate="1">

        <record id="customer_contract_sequence" model="ir.sequence">
            <field name="name">Numérotation des contrats clients</field>
            <field name="active">TRUE</field>
            <field name="code">customer.contract.sequence</field>
            <field name="prefix">CC</field>
            <field name="padding">4</field>
            <field name="suffix">/%(year)s</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="supplier_contract_sequence" model="ir.sequence">
            <field name="name">Numérotation des contrats fournisseurs</field>
            <field name="active">TRUE</field>
            <field name="code">supplier.contract.sequence</field>
            <field name="prefix">CF</field>
            <field name="padding">4</field>
            <field name="suffix">/%(year)s</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

    </data>
</odoo>

